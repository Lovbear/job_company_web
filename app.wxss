  width: 100%;
  height: 300rpx;
  background: #fff;
  font-size: 34rpx;
  min-height:300rpx;
  overflow-y: scroll;
}

.ql-active {
  color: #06c;
}

.ftextrea{
  border: 2rpx solid rgba(151,151,151,1);
  padding:0 30rpx;
  height:400rpx;
  border-radius:10rpx;
}

.fixedadd{
  position: fixed;
  bottom: 20rpx;
  right: 30rpx;
  width: 80rpx;
  height: 80rpx
}
.fixedadd image{
  width: 100%;
  height: 100%;
}

.faceResult{
  margin-top: 30rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:40rpx;
}

.faceResult .right image{
    position: relative;
    top:-2rpx;
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}
.faceResult .right{
   display: inline-block;
   line-height:40rpx;
}

