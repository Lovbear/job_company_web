.myParse .top{
    text-align: center;
    color: #fff;
    background:rgba(34,33,37,1);
}

.myParse .top .s_title{
    padding: 50rpx 0 16rpx;
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
}

.myParse .top .money{
  font-size: 56rpx;
}

.myParse .top .frozen{
    padding: 16rpx 0 45rpx;
    font-size:30rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
}

.myParse .type{
    padding: 40rpx 0;
    font-size:36rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
}

.myParse .type .withdraw{
    margin: 0 auto;
    width:320rpx;
    height:94rpx;
    line-height: 94rpx;
    background:rgba(0,179,138,1);
    border-radius:10rpx;
}

.myParse .type .recharge{
    margin: 0 auto;
    width:320rpx;
    height:94rpx;
    line-height: 94rpx;
    background:#3D97D8;
    border-radius:10rpx;
}

.capital .title{
  position: relative;
  padding: 26rpx 30rpx;
  font-size:30rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}

.capital .title::after{
  content: "";
  position: absolute;
  left: -30rpx;
  right: -30rpx;
  bottom: 0;
  border-bottom: 1px solid rgba(0,0,0,0.15);
}

.capital .recond .item{
    position: relative;
    padding: 36rpx 30rpx 35rpx;
}

.capital .recond .item .item_title{
    font-size:32rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(0,0,0,0.85);
}

.capital .recond .item .item_time{
    margin-top:10rpx; 
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(0,0,0,1);
}

.capital .recond .item .money{
    font-size:36rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(0,0,0,0.85);
}

.capital .recond .item::after{
  content: "";
  position: absolute;
  left:30rpx;
  right:0;
  bottom: 0;
  border-bottom: 1px solid rgba(0,0,0,0.15);
}